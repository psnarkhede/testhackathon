{
  "openapi": "3.0.0",
  "info": {
    "title": "Booking API",
    "version": "ad7b209673a0468c5fb1392a28b13495d49dadc3",
    "description": "API for booking related operations"
  },
  "paths": {
    "/": {
      "get": {
        "summary": "Get Hello World",
        "operationId": "GetHelloWorld",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/bookings": {
      "post": {
        "summary": "Get Bookings",
        "operationId": "GetBookings",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookingRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Booking response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookingResponseDto"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BookingRequestDto": {
        "type": "object",
        "properties": {
          "mobileNumber": {
            "type": "string",
            "description": "Mobile phone number"
          },
          "uuid": {
            "type": "string",
            "format": "uuid",
            "description": "Unique identifier UUID"
          }
        },
        "required": ["mobileNumber", "uuid"]
      },
      "BookingResponseDto": {
        "type": "object",
        "properties": {
          "vehicle": {
            "type": "string",
            "description": "Vehicle type"
          },
          "uuid": {
            "type": "string",
            "format": "uuid",
            "description": "Unique identifier UUID"
          },
          "amount": {
            "type": "number",
            "nullable": true,
            "description": "Amount value"
          },
          "pinode": {
            "type": "number",
            "nullable": true,
            "description": "Pinode value"
          }
        },
        "required": ["vehicle", "uuid"]
      }
    }
  }
}
